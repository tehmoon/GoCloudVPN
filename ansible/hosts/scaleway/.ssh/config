Host *
  ServerAliveInterval 10
  ServerAliveCountMax 3
	Compression yes
	ControlMaster auto
	ControlPersist 60s
	ControlPath hosts/scaleway/.cp/ansible-ssh-%h-%p-%r
	HashKnownHosts yes
	StrictHostKeyChecking no
	UserKnownHostsFile hosts/scaleway/.ssh/known_hosts
	User root
	PreferredAuthentications publickey
